#include "main.h"

/**
 * _isalpha - function that checks for alphabetic character c.
 *
 * @c: Character to be checked
 *
 * Returns 1 if c is a letter, lowercase or uppercase otherwise 0
 */

int _isalpha(int c)

{
	if ((c >= 65 && c <= 90) || (c >= 97 && c <= 122))
	{
	return (1);
	}
	return (0);
}
